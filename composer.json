{
    "name": "vdvt/filter",
    "description": "vdvt filter",
    "authors": [
        {
            "name": "Trinh Le",
            "email": "lehuutrinh32@gmail.com"
        }
    ],
    "homepage": "https://github.com/vdvt/filter",
    "keywords": ["Laravel", "Filter"],
    "require": {
        "illuminate/support": "~7|~8",
        "vdvt/support": "~1.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "VDVT\\Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VDVT\\Filter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VDVT\\Filter\\FilterServiceProvider"
            ],
            "aliases": {
                "Filter": "VDVT\\Filter\\Facades\\Filter"
            }
        }
    }
}
